#!/bin/bash

function usage() {
  cat <<EOF
docker-taglist display tag list.

Usage:
  docker-taglist repo

  - repo        (required) docker repository name. (e.g. golang,library/golang)
EOF
}

[ -z "$1" ] && { usage; exit 1; }
curl -s https://registry.hub.docker.com/v1/repositories/"$1"/tags | jq -r ".[].name" | sort
